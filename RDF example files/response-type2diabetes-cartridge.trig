@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix hao: <http://idea.tw.rpi.edu/projects/heals/hao/> .
@prefix sco: <http://idea.tw.rpi.edu/projects/heals/sco/> .
@prefix obi: <http://purl.obolibrary.org/obo/OBI_> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix stato: <http://purl.obolibrary.org/obo/STATO_> .
@prefix obcs: <http://purl.obolibrary.org/obo/OBCS_> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix chear: <http://hadatac.org/ont/chear#> .
@prefix : <http://idea.tw.rpi.edu/projects/heals/hao/kb/> .


:response-type2diabetes-cartridge-response-type2diabetes-cartridge {

    :study_variable_code-response-type2diabetes-cartridge
        rdf:type    hao:Code ;
        rdfs:label    "Code - diabetes"^^xsd:string ;
        sio:hasValue    "diabetes"^^xsd:string .

    :variable_name-response-type2diabetes-cartridge
        rdf:type    hao:TargetVariable ;
        rdfs:label    "Target Variable - Uncontrolled Diabetes"^^xsd:string ;
        sio:hasValue "uncontrolled diabetes"^^xsd:string .
    
    :data_type-response-type2diabetes-cartridge
        rdf:type    hao:DataType ;
        rdfs:label    "Binary Variable"^^xsd:string ;
        sio:hasValue    [ rdf:type hao:BinaryVariable ] .

    :model_type-response-type2diabetes-cartridge
        rdf:type    sio:Model ;
        rdfs:label    "Model"^^xsd:string ;
        sio:hasValue    [ rdf:type stato:0000149 ] .

    :confounders-response-type2diabetes-cartridge
        rdf:type    obcs:0000039 ;
        rdfs:label    "Confounders"^^xsd:string ;
        sio:hasAttribute :variables-response-type2diabetes-cartridge , 
            :provenance1-response-type2diabetes-cartridge.
    
    :variables-response-type2diabetes-cartridge
        rdf:type   stato:0000258  ;
        rdfs:label    "Variables"^^xsd:string ;
        sio:hasAttribute    [ rdf:type sio:BiologicalSex ] ,
            [ rdf:type chear:BMI ] , 
            [ rdf:type sio:Ethnicity ] , 
            [ rdf:type sco:PovertyIncomeRatio ] , 
            [ rdf:type sio:Age ] .
            
     :provenance1-response-type2diabetes-cartridge
        rdf:type    obcs:0000039 ;
        rdfs:label    "Derived From"^^xsd:string ;
        prov:wasDerivedFrom <https://doi.org/10.1371/journal.pone.0010746> .   


    :definition-response-type2diabetes-cartridge
        rdf:type    sio:Definition ;
        rdfs:label    "Definition"^^xsd:string ;
        sio:hasAttribute :cond1-response-type2diabetes-cartridge , 
            :expression-response-type2diabetes-cartridge , 
            :conjunction-response-type2diabetes-cartridge,
            :n_conds-response-type2diabetes-cartridge ,
            :provenance2-response-type2diabetes-cartridge.

    :cond1-response-type2diabetes-cartridge
        rdf:type    hao:Condition ;
        rdfs:label    "Condition"^^xsd:string ;
        sio:hasAttribute    :lhs-response-type2diabetes-cartridge,
            :relation-response-type2diabetes-cartridge,
            :rhs-response-type2diabetes-cartridge .
     
    :expression-response-type2diabetes-cartridge
        rdf:type    sio:Description  ;
        rdfs:label    "LBXGH >= 7.5"^^xsd:string ;
        sio:hasValue    "LBXGH >= 7.5"^^xsd:string .

    :conjunction-response-type2diabetes-cartridge
        rdf:type    hao:Conjunction ;
        rdfs:label    "Conjunction - max"^^xsd:string ;
        sio:hasValue    "max"^^xsd:string.

    :n_conds-response-type2diabetes-cartridge
        rdf:type    sio:Count ;
        rdfs:label    "Number of Conditions - 1"^^xsd:string ;
        sio:hasValue    "1"^^xsd:integer .

    :provenance2-response-type2diabetes-cartridge
        rdf:type    obcs:0000039 ;
        rdfs:label    "Derived From"^^xsd:string ;
        prov:wasDerivedFrom <https://doi.org/10.2337/dc18-S008> .

    :lhs-response-type2diabetes-cartridge
        rdf:type    hao:LHS ;
        rdfs:label    "Left Hand Side: LBXGH"^^xsd:string ;
        sio:hasValue    "LBXGH"^^xsd:string .

    :relation-response-type2diabetes-cartridge
        rdf:type    hao:Relationship ;
        rdfs:label    "Relation : >="^^xsd:string ;
        sio:hasValue    ">="^^xsd:string .

    :rhs-response-type2diabetes-cartridge
        rdf:type    hao:RHS ;
        rdfs:label    "Right Hand Side: 7.5"^^xsd:string ;
        sio:hasValue    "7.5"^^xsd:float.

    :analysis_question-response-type2diabetes-cartridge
        rdf:type    hao:AnalysisQuestion ;
        rdfs:label    "Analysis Question"^^xsd:string ;
        sio:hasValue    "Are higher amounts of a risk factor significantly associated with the presence of uncontrolled diabetes?"^^xsd:string .
}
